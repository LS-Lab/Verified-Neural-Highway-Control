);

/* Simplificaton for BACK Case */
Bool modelplexPredicateSimplified(Real xf,Real vf,Real xl,Real vl, Real x1post, Real x2post, Real x3post) <-> (

x1post >= x2post & x1post >= x3post |
x2post > x1post & x2post >= x3post & (
(
    -Bmin() <= 0 &
    0 <= Amax() &
    vf >= 0 &
    xf + vf^2 / (2 * Bmin()) + (0 / Bmin() + 1) * (T() * vf) + L()  <
     xl + vl^2 / (2 * Bmax())
)
) |
x3post > x1post & x3post > x2post & (
    (
      -Bmax() <= Amax() &
      Amax() <= -Bmin() &
      xf + vf^2 / (2 * Bmin()) + L()  <  xl + vl^2 / (2 * Bmax()) |
      -Bmin() <= Amax() &
      vf + Amax() * T()  <  0 &
      xf + vf^2 / (2 * (-Amax())) + L()  <  xl + vl^2 / (2 * Bmax()) |
      -Bmin() <= Amax() &
      vf + Amax() * T() >= 0 &
      xf + vf^2 / (2 * Bmin()) +
      (Amax() / Bmin() + 1) * (Amax() / 2 * T()^2 + T() * vf) +
      L()  <
       xl + vl^2 / (2 * Bmax())
    )    
)

);

Bool invariant(Real xf,Real vf, Real af, Real xl,Real vl, Real al, Real t, Real t0) <-> (ctxtConst & invBack(xf,vf,af,xl,vl,al,t,t0));

Bool invariantSimplified(Real xf, Real vf, Real xl, Real vl) <-> (ctxtConst & E(vf,vl) & Hback(xf,vf,xl,vl));

Bool additionalContext(Real xf,Real vf,Real xl,Real vl,Real x1post,Real x2post,Real x3post,Real afpost,Real t0,Real t0post,Real t) <-> (
    /*t0post=t & tpost=t & xfpost=xf & vfpost=vf & xlpost=xl & vlpost=vl &
    -Bmax() <= alpost  &  alpost <= Amax() &*/
    ((x1post >= x2post  &  x1post >= x3post) -> (-Bmax() <= afpost  &  afpost <= -Bmin())) &
    ((x2post > x1post  &  x2post >= x3post) -> afpost = 0) & 
    ((x3post > x1post  &  x3post > x2post) -> afpost = Amax()) &
    Bmax() >= Bmin() & Amax() >= Amin() & Bmin() > 0 & Amin() > 0 & T()>0 & L()>0 & V()>0 &
    xf + L() <= xl & t0post=t & t0 <= t & t <= t0 + T()
);

End.

ProgramVariables
Real xf,vf,af;
Real xl,vl,al;
Real t,t0;
Real x1,x2,x3;
Real x1post,x2post,x3post;
Real xlpost,vlpost,alpost;
Real xfpost,vfpost,afpost;
Real tpost,t0post;
End.

Problem
additionalContext(xf,vf,xl,vl,x1post,x2post,x3post,afpost,t0,t0post,t)
->
(
(invariantSimplified(xf,vf,xl,vl) -> modelplexPredicateSimplified(xf,vf,xl,vl,x1post,x2post,x3post))
<->
(
invariantSimplified(xf,vf,xl,vl) -> modelplexPredicateNotRedundant(xf,vf,xl,vl,t,afpost,x1post,x2post,x3post,t0post)
)
)
End.
Tactic "Proofscript"
unfold;
equivR('R=="invariantSimplified(xf,vf,xl,vl)->modelplexPredicateSimplified(xf,vf,xl,vl,x1post,x2post,x3post)<->invariantSimplified(xf,vf,xl,vl)->modelplexPredicateNotRedundant(xf,vf,xl,vl,t,afpost,x1post,x2post,x3post,t0post)"); <(
  "(invariantSimplified(xf,vf,xl,vl)->modelplexPredicateSimplified(xf,vf,xl,vl,x1post,x2post,x3post))&(invariantSimplified(xf,vf,xl,vl)->modelplexPredicateNotRedundant(xf,vf,xl,vl,t,afpost,x1post,x2post,x3post,t0post))":
    unfold;
    expandAllDefs();
    unfold;
