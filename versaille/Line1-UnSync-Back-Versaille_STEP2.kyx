      afpost <= Amax() &
      xl + (vl - V())^2 / (2 * (-Amax())) + L()  <
       xf + (vf - V())^2 / (2 * (-Amin())) |
      afpost <= Amin() &
      vf + afpost * T() > V() &
      xl + (vl - V())^2 / (2 * (-Amax())) + L()  <
       xf + (vf - V())^2 / (2 * (-afpost)) |
      afpost <= Amin() &
      vf + afpost * T() <= V() &
      xl + (vl - V())^2 / (2 * (-Amax())) + L()  <
       xf + (vf - V())^2 / (2 * (-Amin())) +
      (1 - afpost / Amin()) * (afpost / 2 * T()^2 + T() * (vf - V()))
    )
  ) &
  afpost <= Amax() & t0post = t
) |
(x1post  <  x2post | x1post  <  x3post) &
(
  x2post > x1post &
  x2post >= x3post &
  (
    xf <= xl & -Bmax() <= 0 & 0 <= -Bmin() & afpost = 0 & t0post = t |
    xl <= xf & Amin() <= 0 & 0 <= Amax() & afpost = 0 & t0post = t |
    (
      xf + L() <= xl &
      (
        -Bmax() <= 0 &
        0 <= -Bmin() &
        xf + vf^2 / (2 * Bmin()) + L()  <  xl + vl^2 / (2 * Bmax()) |
        -Bmin() <= 0 &
        0 <= Amax() & vf  <  0 & xf + vf^2 / 0 + L()  <  xl + vl^2 / (2 * Bmax()) |
        -Bmin() <= 0 &
        0 <= Amax() &
        vf >= 0 &
        xf + vf^2 / (2 * Bmin()) + (0 / Bmin() + 1) * (T() * vf) + L()  <
         xl + vl^2 / (2 * Bmax())
      ) |
      xl + L() <= xf &
      (
        Amin() <= 0 &
        0 <= Amax() &
        xl + (vl - V())^2 / (2 * (-Amax())) + L()  <
         xf + (vf - V())^2 / (2 * (-Amin())) |
        -Bmax() <= 0 &
        0 <= Amin() &
        vf > V() &
        xl + (vl - V())^2 / (2 * (-Amax())) + L()  <  xf + (vf - V())^2 / 0 |
        -Bmax() <= 0 &
        0 <= Amin() &
        vf <= V() &
        xl + (vl - V())^2 / (2 * (-Amax())) + L()  <
         xf + (vf - V())^2 / (2 * (-Amin())) +
        (1 - 0 / Amin()) * (T() * (vf - V()))
      )
    ) &
    -Bmax() <= 0 & 0 <= Amax() & afpost = 0 & t0post = t
  ) |
  (x2post <= x1post | x2post  <  x3post) &
  (
    x3post > x1post &
    x3post > x2post &
    Amin() <= afpost &
    afpost <= Amax() &
    (
      xf <= xl & -Bmax() <= afpost & afpost <= -Bmin() & t0post = t |
      xl <= xf & t0post = t |
      (
        xf + L() <= xl &
        (
          -Bmax() <= afpost &
          afpost <= -Bmin() &
          xf + vf^2 / (2 * Bmin()) + L()  <  xl + vl^2 / (2 * Bmax()) |
          -Bmin() <= afpost &
          vf + afpost * T()  <  0 &
          xf + vf^2 / (2 * (-afpost)) + L()  <  xl + vl^2 / (2 * Bmax()) |
          -Bmin() <= afpost &
