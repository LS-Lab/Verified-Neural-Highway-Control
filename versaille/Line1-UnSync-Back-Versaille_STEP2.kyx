          vf + afpost * T() > V() &
          xl + (vl - V())^2 / (2 * (-Amax())) + L()  <
           xf + (vf - V())^2 / (2 * (-afpost)) |
          -Bmax() <= afpost &
          afpost <= Amin() &
          vf + afpost * T() <= V() &
          xl + (vl - V())^2 / (2 * (-Amax())) + L()  <
           xf + (vf - V())^2 / (2 * (-Amin())) +
