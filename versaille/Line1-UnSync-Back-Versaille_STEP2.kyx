          L()  <
           xl + vl^2 / (2 * Bmax())
        ) |
        xl + L() <= xf &
        (
          xl + (vl - V())^2 / (2 * (-Amax())) + L()  <
           xf + (vf - V())^2 / (2 * (-Amin())) |
